.dropdown {
	position: relative;
	display: inline-block;
	margin-right: 50px;
}

.dropdown-content {
	display: none;
	position: absolute;
	background-color: #5340ff;
	min-width: 160px;
	/* box-shadow: 0px 8px 16px 0px rgb(0 0 0 / 20%);
     */
